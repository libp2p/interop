{
  "name": "interop-libp2p",
  "version": "0.4.1",
  "description": "Interoperability Tests for libp2p",
  "leadMaintainer": "Vasco Santos <santos.vasco10@gmail.com>",
  "main": "",
  "files": [
    "test",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">6.0.0"
  },
  "scripts": {
    "lint": "aegir lint",
    "test": "aegir test -t node -f test/node.js -- --exit",
    "test:node": "aegir test -t node -f test/node.js -- --exit",
    "test:browser": "aegir test -t browser --no-cors -f test/browser.js",
    "prerelease": "npm run test",
    "release": "aegir release --target node --build=false --no-test",
    "release-minor": "aegir release --type minor --target node  --build=false --no-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/interop.git"
  },
  "keywords": [
    "libp2p"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libp2p/interop/issues"
  },
  "homepage": "https://github.com/libp2p/interop#readme",
  "devDependencies": {
    "aegir": "^35.0.2",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "chai-checkmark": "^1.0.1",
    "cross-env": "^7.0.0",
    "dirty-chai": "^2.0.1",
    "go-libp2p-dep": "^0.11.0",
    "libp2p-daemon": "^0.7.0",
    "libp2p-daemon-client": "^0.7.0",
    "multiaddr": "^10.0.1",
    "p-retry": "^4.2.0",
    "rimraf": "^3.0.0"
  },
