    "Steven Allen <steven@stebalien.com>",
    "Mantas Vidutis <mantas.a.vidutis@gmail.com>"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "execa": "^4.0.0",
    "multiformats": "^9.4.5",
    "uint8arrays": "^3.0.0"
  }
}
